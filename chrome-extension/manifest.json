{
  "name": "Chrome Litchi Virtual Mission",
  "version": "0.2.1",
  "description": "Adds ability to export mission from mission hub as google earth virtual tour",
  "content_scripts": [
    {
      "matches": ["https://flylitchi.com/hub*"],
      "js": ["hook.js"]
    }
  ],
  "options_ui": { "page": "options.html", "open_in_tab": true },
  "permissions": [ "storage" ],
  "icons": { "128": "cevlm.png" },
  "manifest_version": 2
}
